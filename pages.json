{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue",
		"^sxf-(.*)": "@/components/$1/$1.vue"
	},
	"tabBar": {
		"color": "#ccc",
		"selectedColor": "#FA6400",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"position": "bottom",
